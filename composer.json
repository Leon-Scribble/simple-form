    "extra": {
        "handle": "simple-form",
        "name": "SimpleForm",
        "developer": "Leon",
        "documentationUrl": "",
        "class": "scribblewerbeagentur\\craftsimpleform\\SimpleForm"
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

